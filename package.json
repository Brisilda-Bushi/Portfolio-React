{
  "name": "portfolio-react",
  "version": "0.1.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "i18n-file": "i18next-scanner && cat src/translations/en.json >tmp && mv tmp src/translations/en.json||exit 0",
    "update-translations": "node src/utils/update-translations.mjs ",
    "check:translation": "yarn run i18n-file && yarn run update-translations",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.jsx' --write"
  },
  "dependencies": {
    "@emailjs/browser": "^3.11.0",
    "@react-three/drei": "^9.115.0",
    "@react-three/fiber": "^8.13.5",
    "@types/three": "^0.160.0",
    "dotenv": "^16.3.1",
    "framer-motion": "^10.12.21",
    "i18next": "^23.7.18",
    "i18next-http-backend": "^2.6.1",
    "maath": "^0.7.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.1",
    "react-parallax-tilt": "^1.7.153",
    "react-router-dom": "^6.14.1",
    "react-toastify": "^9.1.3",
    "react-vertical-timeline-component": "^3.6.0",
    "three": "^0.154.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "i18next-scanner": "^4.4.0",
    "postcss": "^8.4.26",
    "tailwindcss": "^3.3.3",
    "vite": "^4.4.0",
    "yarn-audit-fix": "^10.1.0"
  }
}
